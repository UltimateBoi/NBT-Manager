                <TabItem.Header>
                    <Grid
                        x:Name="Tab3HeaderGrid">
                        <TextBlock
                            Text="Items"
                            MinWidth="90"
                            TextAlignment="Center" Foreground="White" />
                    </Grid>
                </TabItem.Header>
                <Grid
                    x:Name="Tab3Grid"
                    Background="Black">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition
                            Width="*" />
                        <ColumnDefinition
                            Width="*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                        <RowDefinition
                            Height="*" />
                    </Grid.RowDefinitions>
                    <TextBox
                        x:Name="ShulkerTextBox"
                        HorizontalAlignment="Stretch"
                        TextWrapping="Wrap"
                        Text="&lt;Enter data value here&gt;"
                        VerticalContentAlignment="Center"
                        VerticalAlignment="Stretch"
                        Margin="100,10,10,10"
                        TextAlignment="Center"
                        ToolTip="" Foreground="White" />
                    <Button
                        x:Name="ShulkerButton"
                        Content="Get Shulker Box"
                        Grid.Column="1"
                        HorizontalAlignment="Stretch"
                        Margin="10"
                        VerticalAlignment="Stretch"
                        ToolTip=".give shulker_box 1 0 .nbt write"
                        Click="ShulkerButton_Click"
                        PreviewMouseDown="Button_PreviewMouseDown" />
                    <TextBlock
                        Margin="10,20,0,10"
                        HorizontalAlignment="Left"><Hyperlink
                            NavigateUri="https://minecraft.gamepedia.com/Bedrock_Edition_data_values"
                            RequestNavigate="Hyperlink_RequestNavigate"><Run Text="Data values"/></Hyperlink></TextBlock>
                </Grid>
            </TabItem>
        </TabControl>
        <Label Content="Manager" HorizontalAlignment="Left" Margin="60,15,0,0" VerticalAlignment="Top" Width="115" Foreground="#DDFFFFFF" Height="45" FontSize="24" MouseDown="Drag"/>
        <Rectangle HorizontalAlignment="Left" Height="40" Margin="15,15,0,0" Stroke="Black" VerticalAlignment="Top" Width="40" RadiusY="4.5" RadiusX="4.5">
            <Rectangle.Fill>
                <ImageBrush ImageSource="NBT Manager.png" Stretch="UniformToFill"/>
            </Rectangle.Fill>
        </Rectangle>
        <Button x:Name="Close" Content="&#xE8BB;" materialDesign:RippleAssist.RippleSizeMultiplier="0.7" HorizontalAlignment="Left" Height="35" Padding="0" Style="{DynamicResource MaterialDesignToolButton}" Margin="715,20,0,0" VerticalAlignment="Top" Width="35" Foreground="White" FontFamily="Segoe MDL2 Assets" Click="Exit"/>
        <Button x:Name="Minimise" Content="&#xE921;" materialDesign:RippleAssist.RippleSizeMultiplier="0.7" HorizontalAlignment="Left" Height="35" Padding="0" Style="{DynamicResource MaterialDesignToolButton}" Margin="675,20,0,0" VerticalAlignment="Top" Width="35" Foreground="White" FontFamily="Segoe MDL2 Assets" Click="Minimize"/>
    </Grid>
</Window>
